ddress.address}, {cart.shippingAddress.city}\n                  {\"    \"} ,{cart.shippingAddress.state1} {\"    \"}{\" \"}\n                  {cart.shippingAddress.zipCode}\n                  {\"    \"} {cart.shippingAddress.country}\n                </p>\n              </List.Item>\n              <Divider />\n              <List.Item>\n                <h2>PAYMENT METHOD:</h2>\n                <p>\n                  <strong>Payment Selected: </strong>\n                  {cart.paymentMethod}\n                </p>\n              </List.Item>\n              <Divider />\n              <List.Item>\n                <h2>ORDER ITEMS:</h2>\n                {cart.cartItems.length === 0 ? (\n                  <MessageAlert color=\"red\">Your cart is empty!</MessageAlert>\n                ) : (\n                  <List>\n                    {cart.cartItems.map((item, index) => (\n                      <List.Item key={index}>\n                        <Grid celled>\n                          <Grid.Row>\n                            <Grid.Column width={4}>\n                              <Image\n                                size=\"small\"\n                                src={item.image}\n                                alt={item.name}\n                                rounded\n                                centered\n                              />\n                            </Grid.Column>\n                            <Grid.Column width={6}>\n                              <Link to={`/collection/${item.id}`}>\n                                <h3>{item.name}</h3>\n                              </Link>\n                            </Grid.Column>\n                            <Grid.Column width={6}>\n                              <h3>\n                                {item.quantity} X $ ${item.price} = $\n                                {(item.quantity * item.price).toFixed(2)}\n                              </h3>\n                            </Grid.Column>\n                          </Grid.Row>\n                        </Grid>\n                      </List.Item>\n                    ))}\n                  </List>\n                )}\n              </List.Item>\n            </List>\n          </Grid.Column>\n\n          <Grid.Column width={5}>\n            <Card fluid>\n              <h2>ORDER SUMMARY</h2>\n              <List>\n                <List.Item>\n                  <Grid celled>\n                    <Grid.Row>\n                      <Grid.Column width={8}>\n                        <h3>ITEM:</h3>\n                      </Grid.Column>\n                      <Grid.Column width={8}>\n                        <h4>${cart.itemsPrice}</h4>\n                      </Grid.Column>\n                    </Grid.Row>\n                    <Grid.Row>\n                      <Grid.Column width={8}>\n                        <h3>SHIPPING:</h3>\n                      </Grid.Column>\n                      <Grid.Column width={8}>\n                        <h4>${cart.shippingPrice}</h4>\n                      </Grid.Column>\n                    </Grid.Row>\n                    <Grid.Row>\n                      <Grid.Column width={8}>\n                        <h3>TAX:</h3>\n                      </Grid.Column>\n                      <Grid.Column width={8}>\n                        <h4>${cart.tax}</h4>\n                      </Grid.Column>\n                    </Grid.Row>\n                    <Grid.Row>\n                      <Grid.Column width={8}>\n                        <h3>TOTAL:</h3>\n                      </Grid.Column>\n                      <Grid.Column width={8}>\n                        <h4>${cart.totalPrice}</h4>\n                      </Grid.Column>\n                    </Grid.Row>\n                  </Grid>\n                </List.Item>\n\n                <List.Item>\n                  {error && <MessageAlert color=\"red\">{error}</MessageAlert>}\n                </List.Item>\n\n                <List.Item>\n                  <Button\n                    type=\"button\"\n                    disabled={cart.cartItems.length === 0}\n                    fluid\n                    onClick={placeOrderHandler}\n                  >\n                    CONFIRM ORDER\n                  </Button>\n                </List.Item>\n              </List>\n            </Card>\n          </Grid.Column>\n        </Grid.Row>\n      </Grid>\n    </div>\n  );\n}\n\nexport default withRouter(PlaceOrderPage);\n","/Users/krystinexie/Documents/CodingDojo/MERN/MERN-Project-2021/au_jardin_de_may/client/src/reducers/orderReducers.js",[],"/Users/krystinexie/Documents/CodingDojo/MERN/MERN-Project-2021/au_jardin_de_may/client/src/constants/orderConstants.js",[],"/Users/krystinexie/Documents/CodingDojo/MERN/MERN-Project-2021/au_jardin_de_may/client/src/actions/orderActions.js",[],"/Users/krystinexie/Documents/CodingDojo/MERN/MERN-Project-2021/au_jardin_de_may/client/src/constants/states.js",[],"/Users/krystinexie/Documents/CodingDojo/MERN/MERN-Project-2021/au_jardin_de_may/client/src/components/SearchBar.js",[],{"ruleId":"193","replacedBy":"194"},{"ruleId":"195","replacedBy":"196"},{"ruleId":"193","replacedBy":"197"},{"ruleId":"195","replacedBy":"198"},{"ruleId":"199","severity":1,"message":"200","line":16,"column":3,"nodeType":"201","messageId":"202","endLine":16,"endColumn":28},{"ruleId":"199","severity":1,"message":"203","line":19,"column":10,"nodeType":"201","messageId":"202","endLine":19,"endColumn":18},{"ruleId":"204","severity":1,"message":"205","line":38,"column":6,"nodeType":"206","endLine":38,"endColumn":24,"suggestions":"207"},"no-native-reassign",["208"],"no-negated-in-lhs",["209"],["208"],["209"],"no-unused-vars","'USER_PROFILE_UPDATE_RESET' is defined but never used.","Identifier","unusedVar","'getState' is defined but never used.","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'order._id'. Either include it or remove the dependency array.","ArrayExpression",["210"],"no-global-assign","no-unsafe-